<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/four_in_a_row_column_first"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/ic_spooky_fourinarow_playgroundelement"
        app:layout_constraintBottom_toTopOf="@+id/four_in_a_row_column_second"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.06"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/four_in_a_row_column_second"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/ic_spooky_fourinarow_playgroundelement"
        app:layout_constraintBottom_toTopOf="@+id/four_in_a_row_column_third"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.06"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/four_in_a_row_column_first" />

    <ImageView
        android:id="@+id/four_in_a_row_column_third"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/ic_spooky_fourinarow_playgroundelement"
        app:layout_constraintBottom_toTopOf="@+id/four_in_a_row_column_fourth"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.06"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/four_in_a_row_column_second" />

    <ImageView
        android:id="@+id/four_in_a_row_column_fourth"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/ic_spooky_fourinarow_playgroundelement"
        app:layout_constraintBottom_toTopOf="@+id/four_in_a_row_column_fifth"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.06"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/four_in_a_row_column_third" />

    <ImageView
        android:id="@+id/four_in_a_row_column_fifth"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/ic_spooky_fourinarow_playgroundelement"
        app:layout_constraintBottom_toTopOf="@+id/four_in_a_row_column_sixth"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.06"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/four_in_a_row_column_fourth" />

    <ImageView
        android:id="@+id/four_in_a_row_column_sixth"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/ic_spooky_fourinarow_playgroundelement"
        app:layout_constraintBottom_toBottomOf="@+id/four_in_a_row_end_guideline"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.06"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/four_in_a_row_column_fifth" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/four_in_a_row_end_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.36" />
</androidx.constraintlayout.widget.ConstraintLayout>